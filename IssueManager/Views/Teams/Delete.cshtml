@model IssueManager.Models.Team

@{
    ViewData["Title"] = "Delete";
}

<h3>Are you sure you want to delete Team: @Model.Name?</h3>
<div>    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
